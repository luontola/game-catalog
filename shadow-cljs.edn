{:source-paths ["src"]
 :dependencies [[funcool/promesa "8.0.450"]
                [reagent "1.1.1"]]
 :dev-http {8080 "public"}

 :builds
 {:frontend {:target :browser
             :output-dir "public/js"
             :modules {:main {:init-fn game-catalog.ui/init!}}
