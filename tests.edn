#kaocha/v1
{:tests [{:id :fast
          :skip-meta [:slow]
          :source-paths ["src" "resources"]
          :test-paths ["src"]
          :ns-patterns ["^game-catalog"]}
         {:id :slow
          :focus-meta [:slow]
          :source-paths ["src" "resources"]
          :test-paths ["src"]
          :ns-patterns ["^game-catalog"]}]

 :reporter [kaocha.report.progress/report]
 :plugins [:kaocha.plugin/profiling]
 :kaocha.plugin.profiling/count 3
 :capture-output? true}
